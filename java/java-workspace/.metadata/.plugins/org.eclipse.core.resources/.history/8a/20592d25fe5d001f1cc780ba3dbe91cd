package main;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
	
		int a, b, sum = 0;
		
		for(int i = 1; i <= num; i++) {
			a = input.nextInt();
			b = input.nextInt();
			sum = a + b;
			System.out.println("Case #" + i + ": " + sum);
		}
	}
}