package Quiz;
//16장.상속.pdf 강의교안의 31쪽 8번문제

class A {
	public A() {
		System.out.println("hello from a");
	}
}

class B extends A{
	public B() {
		
		System.out.println("hell from b");
//		super(); // 에러남. 부모생성자를 먼저 호출해야됨
		
	}
}
public class Ex16_09 {
	public static void main(String[] args) {
		A a = new B();
	}
}
